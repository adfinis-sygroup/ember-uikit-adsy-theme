  {{/nav.left}}
  {{#nav.right}}
    {{#nav.brand 'getting-started.index' logo=false}}
      ember-uikit-adsy-theme
    {{/nav.brand}}
  {{/nav.right}}
{{/uk-adsy-topnav}}

<div class="uk-grid">
  {{#uk-adsy-sidenav muted=true as |nav|}}
