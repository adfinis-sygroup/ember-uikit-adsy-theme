    {{#nav.list as |list|}}
      {{#list.item header=true}}Components{{/list.item}}
      {{#list.item 'components.sidenav'}}Sidenav{{/list.item}}
      {{#list.item 'components.colors'}}Colors{{/list.item}}
    {{/nav.list}}
  {{/uk-adsy-sidenav}}

  <div class="uk-padding">
    {{outlet}}
